/* pages/home/home.wxss */
page {
  display: block;
  min-height: 100%;
  min-width: 100%;
  background-color: #002C40;
} 
.titleview{
  margin: 20rpx;
  margin-bottom: 0rpx;
}
.mytitle {
  font-size: 40rpx;
  color: #ffffff;
}

.view1 {
  margin: auto;
  width: 100%;
  height: 100%;
  text-align: center;
}

.view_text {
  width: 100%;
  height: 60rpx;
  margin-top: -4rpx;
}

.image1 {
  width: 100%;
  height: 60rpx;
  margin-top: 10rpx;
}

.mytext {
  color: #fff;
  font-size: 28rpx;
}

.view_row {
  width: 95%;
  height: 120rpx;
  margin: auto;
  margin-top: 20rpx;
  display: flex;
}
.tabBarimage{
  width: 100%;
  height: 40rpx;
  margin-top: 10rpx;
}

.swiper-tab{
    width: 100%;
    text-align: center;
    height: 100rpx;
    display: flex;
    position:fixed; 
    bottom:0;
    z-index: 2000;
}
.swiper-tab-item{
    width: 25%; 
}
.active{
    color:#F65959;
    border-bottom: 4rpx solid #F65959;
}
